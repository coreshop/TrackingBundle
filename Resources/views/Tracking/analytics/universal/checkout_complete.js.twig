{% set method = isGoogleTagMode == true ? 'gtag' : 'ga' %}
{% if isGoogleTagMode == false %}
    {{ method }}('require', 'ecommerce', 'ecommerce.js');
{% endif %}

{% for key, call in calls %}
    {% for c in call %}
        {{ method }}('{{ key }}', {{ c|json_encode()|raw }});
    {% endfor %}
{% endfor %}

{{ method }}('ecommerce:send');